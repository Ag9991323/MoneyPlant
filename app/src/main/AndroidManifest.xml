<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.networkmarketing">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="MoneyPlant"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name=".OfflinePurposes"
        android:theme="@style/AppTheme">
        <activity android:name=".childShowActivity"></activity>
        <activity android:name=".OwnerDashboardActivity" />
        <activity android:name=".splashscreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RequestActivity" />
        <activity android:name=".ChildActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".Dashboard"></activity>
        <activity android:name=".VerifyPhoneActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>